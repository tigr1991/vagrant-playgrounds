options {
        directory "/var/cache/bind";
        listen-on {
            192.168.1.0/24;
            127.0.0.1;
        };

        forwarders {
            10.20.6.254;
        };

        dnssec-validation auto;

        auth-nxdomain no;

        listen-on-v6 {
            all;
        };

        version "DNS server v1.0";

};
